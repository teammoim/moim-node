
<head>
  <script>
    var msg = "<%= msg %>";
    if (msg === "loginErr") {
      alert("Invalid Login");
      location.href = "/login";
    }
    else if (msg === "emailUsed") {
      alert("Email is already used!");
      location.href = "/signUp"; 
    }
    else if (msg === "signUpErr") {
      alert("invalid SignUp");
      location.href = "/signUp";
    }
  </script>
</head>