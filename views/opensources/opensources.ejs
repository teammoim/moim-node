<% include ../core %>

<body>
    <style>
        .osbox-container {
            display: flex;
            height: 220px;
            margin-left: 10%;
            margin-right: 10%;
            padding-top: 20px;
        }

        .osbox {
            position: relative;
            padding: 5px;
            display: inline-block;
            width: 25% ;
            height: 100%;
            margin: 10px;
            font-size: 15px;
            overflow: scroll;
        }
        .linkbar-left{
            position: absolute; 
            bottom: 5px; 
            width: 50%;
            height: 10%;
            left: 30px; 
            margin-right: 50%; 
            text-align: left;
            font-size: 20px;
        }
        .linkbar-right{
            position: absolute; 
            bottom: 5px; 
            width: 50%;
            height: 10%;
            right: 30px; 
            margin-left: 50%; 
            text-align: right;
            font-size: 18px;
        }
